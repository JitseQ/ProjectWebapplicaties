@{
    ViewData["Title"] = "Line-up";
}
@model FestivalApplication.ViewModels.LineUpListViewModel

<div class="text-center">
    <h1 class="display-4">Line-Up</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a></p>

    @foreach(var item in Model.ArtiestDatums)
		{
			<tr>
				<td class="col-md-4">
					@item.ArtiestId
				</td>
				
				<td class="col-md-4">
					<a asp-controller="Artiest" asp-action="Details" asp-route-id="@item.ArtiestId">Details</a>

				</td>
				<td class="col-md-4">
					Edit
				</td>
				<td class="col-md-4">
					Delete
				</td>

			</tr>
		}
</div>
